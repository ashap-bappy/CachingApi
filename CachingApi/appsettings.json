{
  "CacheSettings": {
    "CacheType": "Memcached", // Change to Redis or Memcached based on desired cache
    "Redis": {
      "ConnectionString": "localhost:6379",
      "DefaultExpirationTime": 5
    },
    "Memcached": {
      "Server": {
        "Address": "localhost",
        "Port": 11211
      },
      "DefaultExpirationTime": 5
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
